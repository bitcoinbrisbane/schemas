{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "../../../schemas/health/measurement/schema.json",
  "title": "Measurements",
  "description": "The specific type of the health item",
  "type": "object",
  "properties": {
    "img": {
      "type": "string"
    },
    "name": {
      "title": "Name",
      "type": "string",
      "minLength": 2,
      "maxLength": 60
    },
    "metric": {
      "$ref": "definitions/unit.json#/measure"
    },
    "dateOfMeasurement": {
      "title": "Date of Measurement",
      "type": "string"
    }
  },
  "required": ["name", "dateOfMeasurement"]
}